/* side */

#fp-nav ul li a span, .fp-slidesNav ul li a span {border: 1px solid #999 !important;}
.side{position:fixed;width:54px;height:275px;right:0;top:250px;z-index:100;}
.side ul li{width:54px;height:54px;float:left;position:relative;border-bottom:1px solid #005881;}
.side ul li .sidebox{position:absolute;width:54px;height:54px;top:0;right:0;transition:all 0.3s;background:#539AC6;opacity:1;filter:Alpha(opacity=100);color:#fff;font:14px/54px "微软雅黑";overflow:hidden;}
.side ul li .sidetop{width:54px;height:54px;line-height:54px;display:inline-block;background:#539AC6;opacity:1;filter:Alpha(opacity=100);transition:all 0.3s;}
.side ul li .sidetop:hover{background:#41872F;opacity:1;filter:Alpha(opacity=100);}
.side ul li img{float:left;}

